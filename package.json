{
  "name": "admin",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:push": "npx prisma db push",
    "db:push-f": "npx prisma db push --accept-data-loss",
    "merge-schema": "npx prisma-merge -b prisma/base.prisma -o prisma/schema.prisma -s ./src/models/**/*.schema.prisma",
    "prepare:db": "npm run merge-schema && npx prisma format",
    "pre-generate": "npm run prepare:db",
    "mig:create": "npm run mig:save",
    "mig:save": "npx prisma migrate save",
    "mig:up": "npx prisma migrate up",
    "mig:down": "npx prisma migrate down",
    "dev": "nodemon",
    "first-deploy": "npm install @prisma/client && npm run pre-generate && prisma generate",
    "prestart": "npm run pre-generate && prisma generate",
    "start": "ts-node ./src/server/index.ts",
    "start:prod": "npm run build && node build/index.js",
    "build": "rimraf ./build && tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.30.3",
    "amqplib": "^0.8.0",
    "bluebird": "^3.7.2",
    "cores": "^0.8.5",
    "cors": "^2.8.5",
    "data-forge": "^1.8.20",
    "data-forge-fs": "^0.0.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "node-schedule": "^2.0.0",
    "papaparse": "^5.3.1",
    "rascal": "^13.1.3",
    "socket.io": "^4.3.1",
    "wait-port": "^0.2.9",
    "yup": "^0.32.11",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.1",
    "@types/bluebird": "^3.5.36",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.14",
    "@types/papaparse": "^5.3.1",
    "nodemon": "^2.0.12",
    "prisma": "^3.4.0",
    "prisma-merge": "^0.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  }
}
